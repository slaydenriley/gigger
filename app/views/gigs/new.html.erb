<h1>Add a New Gig!</h1>
<%= form_for @gig do |f| %>
  <%= f.label :name %><br/>
  <%= f.text_field :name %><br/>
  <br/>
  <%= f.label :band_id, "Band" %><br/>
  <%= f.email_field :band_id %><br/>
  <br/>
  <%= f.label :venue_id, "Venue" %><br/>
  <%= f.text_area :venue_id %><br/>
  <br/>
  <%= f.label :date %>
  <%= f.date_select :date %>
  <br/>
  <%= f.submit "Create Gig" %>
<% end %>
