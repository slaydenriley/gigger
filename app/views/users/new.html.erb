<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br/>
  <%= f.label :email %>
  <%= f.email_field :email %><br/>
  <%= f.label :description %>
  <%= f.text_area :description%><br/>
  <%= f.label :band %>
  <%= f.collection_select :band_id, Band.all, :id, :name %>
  <%= f.label :password %>
  <%= f.password_field :password %><br/>
  <%= f.submit %>
  <%= %>



<% end %>
