<h1>Create an Account</h1>
<%= form_for @user do |f| %>
  <%= f.label :name %><br/>
  <%= f.text_field :name %><br/>
  <br/>
  <%= f.label :email %><br/>
  <%= f.email_field :email %><br/>
  <br/>
  <%= f.label :description %><br/>
  <%= f.text_area :description%><br/>
  <br/>
  <%= f.label :type %><br/>
  <%= f.select :type, ["Band Member", "Venue Manager"], prompt: 'Select', id:'band_or_venue' %><br/>
  <br/>
  <%= f.label :band_id, class:'bands' %></br>
  <%= f.collection_select :band_id, Band.all, :id, :name, class:'bands' %><br/>
  <br/>
  <%= f.label :venue_id, class:'venues' %></br>
  <%= f.collection_select :venue_id, Venue.all, :id, :name, class:'venues' %><br/>
  <br/>
  <%= f.label :password %><br/>
  <%= f.password_field :password %><br/>
  <br/>
  <%= f.submit "Create Account" %>
<% end %>




