<%= form_for Band.new do |f| %>
  <%= f.label :name %><br/>
  <%= f.text_field :name %><br/>
  <br/>
  <%= f.label :email %><br/>
  <%= f.email_field :email %><br/>
  <br/>
  <%= f.label :description %><br/>
  <%= f.text_area :description %><br/>
  <br/>
  <%= f.label :users %><br/>
  <%= f.collection_check_boxes :users, User.all, :id, :name, include_hidden: false %>
  </br>
  <br/>
  <%= f.submit "Create Band" %>
<% end %>
