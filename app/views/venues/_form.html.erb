<%= form_for @venue do |f| %>
  <%= f.label :name %><br/>
  <%= f.text_field :name %><br/>
  <br/>
  <%= f.label :email %><br/>
  <%= f.email_field :email %><br/>
  <br/>
  <%= f.label :description %><br/>
  <%= f.text_area :description %><br/>
  <br/>
  <br/>
  <%= f.submit "Create Venue" %>
<% end %>
