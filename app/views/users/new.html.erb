<h1>Create an Account</h1>
<%= form_for User.new do |f| %>
  <%= f.label :name %><br/>
  <%= f.text_field :name %><br/>
  <br/>
  <%= f.label :email %><br/>
  <%= f.email_field :email %><br/>
  <br/>
  <%= f.label :description %><br/>
  <%= f.text_area :description%><br/>
  <br/>
  <%= f.label :password %><br/>
  <%= f.password_field :password %><br/>
  <br/>
  <%= f.submit "Create Account" %>
<% end %>




